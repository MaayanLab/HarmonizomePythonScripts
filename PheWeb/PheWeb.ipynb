    "import json\n",
    "\n",
    "with open('data.json') as o:\n",
    "    data = json.loads(o.read())\n",
    "\n",
    "\n",
    "f = open(\"pheweb.gmt\", \"w\")\n",
    "for json in data:\n",
    "    f.write(str(json[\"phenostring\"]) + '\\t\\t' + str(json[\"nearest_genes\"]) + \"\\n\")\n",
    "\n",
    "\n"
